<h1>Users#index</h1>


<p id="error"><%= flash[:error] %>


<%= form_tag("/", method: "post") do %>
	<%= label_tag(:user, "Username:") %>
	<%= text_field_tag(:user) %>
	<%= label_tag(:password, "Password:") %>
	<%= text_field_tag(:password) %>
	<%= submit_tag("Log In", :onclick => "this.form.action = '/users/login'") %>
	<%= submit_tag("Sign Up", :onclick => "this.form.action = '/users/add'") %>
<% end %>

<p>Find me in app/views/users/index.html.erb</p>
